{
    "name": "FredyHenao/lumen-form-request-url",
    "type": "library",
    "description": "Laravel Form Request adaptation for Lumen framework.",
    "keywords": [
        "lumen",
        "laravel",
        "form request",
        "validation request",
        "lumen form request"
    ],
    "homepage": "https://github.com/FredyHenao/lumen-form-request-url",
    "license": "MIT",
    "authors": [
        {
            "name": "Fredy Henao",
            "email": "fredyhenao45y@gmail.com"
        }
    ],
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "FredyHenao\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
